    "@types/seedrandom": "^3.0.8",
    "next": "^14.0.4",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.14",
    "@types/node": "20.10.8",
    "argparse": "^2.0.1",
